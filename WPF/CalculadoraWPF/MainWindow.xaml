<Window x:Class="CalculadoraWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CalculadoraWPF"
        xmlns:y="clr-namespace:CalculadoraWPF.Views"
        mc:Ignorable="d"
        Title="MainWindow" MinHeight="530" MaxHeight="800" MinWidth="548.933" MaxWidth="1200">
    <Grid Margin="0,0,1.6,-0.4">
        <y:ucLogin x:Name="ucLoginForm" loginCorrect="UcLoginForm_loginCorrect"></y:ucLogin>
        <y:ucCalculadora x:Name="ucCalculadoraForm" Visibility="Hidden"></y:ucCalculadora>
    </Grid>
</Window>
